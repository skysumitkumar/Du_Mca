document.write('<a id="chatterpopup" href="http://chatter.flooble.com/chatter.php?id=perplexus&sid=8&popup=true&w=495&h=400&name=Visitor&title=&nopost=Visitor posting is disabled" target="flooblechatterbox" onClick="window.open(\'about:blank\',\'flooblechatterbox\',\'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,copyhistory=no,width=495,height=400\');return true;"><img src="http://img.flooble.com/images/chatter/popup.gif" border="0" alt="pop it up!"></a>');
document.write(' <a href="http://chatter.flooble.com/hist.php?id=8" target="_blank"><img src="http://img.flooble.com/images/chatter/hist.gif" border="0" alt="Previous Messages" title="Previous Messages"></a>');
