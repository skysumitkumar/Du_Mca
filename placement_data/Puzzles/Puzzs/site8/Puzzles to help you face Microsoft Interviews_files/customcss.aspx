#header {
	PADDING-RIGHT: 10px; BACKGROUND-POSITION: center 50%; PADDING-LEFT: 10px; BACKGROUND-IMAGE: url(http://www.mvpblog.com/pics/bluegrad.gif); PADDING-BOTTOM: 15px; MARGIN: 0px; COLOR: #000000; PADDING-TOP: 0.2em; BORDER-BOTTOM: medium none; BACKGROUND-REPEAT: repeat-y; HEIGHT: 65px; BACKGROUND-COLOR: #2765ab
}
#header A {
	COLOR: #fff
}
#header A:visited {
	COLOR: #fff
}
A {
	COLOR: #1f5ba4
}
A:link {
	COLOR: #1f5ba4
}
A:visited {
	COLOR: #639
}
A:hover {
	COLOR: #f60
}
A IMG {
	BORDER-RIGHT: medium none; BORDER-TOP: medium none; BORDER-LEFT: medium none; BORDER-BOTTOM: medium none
}
H1.block_title {
	PADDING-RIGHT: 10px; BACKGROUND-POSITION: left bottom; PADDING-LEFT: 0px; FONT-WEIGHT: bold; FONT-SIZE: 10pt; BACKGROUND-IMAGE: url(http://www.mvpblog.com/pics/bluegrad.gif); PADDING-BOTTOM: 3px; MARGIN: 10px 0px 0px; TEXT-TRANSFORM: none; COLOR: #555555; PADDING-TOP: 0px; BACKGROUND-REPEAT: no-repeat; FONT-FAMILY: "Franklin Gothic Book", "trebuchet ms" , "lucida grande" , verdana, arial, sans-serif; HEIGHT: 0px
}
DIV.block {
	BACKGROUND-POSITION: left top; FONT-SIZE: 8pt; BACKGROUND-IMAGE: none; MARGIN-RIGHT: 10px; BACKGROUND-REPEAT: repeat-y; TEXT-ALIGN: justify
}
DIV.block_footer {
	BACKGROUND-POSITION: left top; BACKGROUND-IMAGE: none; BACKGROUND-REPEAT: no-repeat; HEIGHT: 10px
}
DIV.seperator {
	BACKGROUND-POSITION: left top; BACKGROUND-IMAGE: none; MARGIN: 15px 0px 10px; BACKGROUND-REPEAT: no-repeat
}
DIV.posttitle {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; FONT-WEIGHT: bold; FONT-SIZE: 2em; PADDING-BOTTOM: 0px; MARGIN: 0px; COLOR: #355ea0; PADDING-TOP: 10px; FONT-FAMILY: "Franklin Gothic Book", "trebuchet ms" , "lucida grande" , verdana, arial, sans-serif
}
DIV.itemdesc A {
	TEXT-TRANSFORM: none; COLOR: #555555
}
DIV.itemdesc A:visited {
	TEXT-TRANSFORM: none; COLOR: #555555
}
#menu H1 {
	FONT-WEIGHT: bold; FONT-SIZE: 12pt; MARGIN: 0px; COLOR: #000066; FONT-FAMILY: "trebuchet ms" , "lucida grande" , verdana, arial, sans-serif
}
DIV.listtitle {
	FONT-WEIGHT: bold; FONT-SIZE: 12pt; MARGIN: 0px; COLOR: #000066; FONT-FAMILY: "trebuchet ms" , "lucida grande" , verdana, arial, sans-serif
}
#menu UL {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; FONT-SIZE: 8pt; PADDING-BOTTOM: 0px; MARGIN: 0px 0px 15px; COLOR: #1f5ba4; PADDING-TOP: 0px; LIST-STYLE-TYPE: none
}
DIV#header {
	PADDING-LEFT: 15px
}
DIV#menu {
	PADDING-LEFT: 15px
}
H1.block_title A {
	PADDING-RIGHT: 0px; BACKGROUND-POSITION: left top; PADDING-LEFT: 0px; FONT-WEIGHT: bold; FONT-SIZE: 1em; BACKGROUND-IMAGE: none; PADDING-BOTTOM: 3px; MARGIN: 0px; TEXT-TRANSFORM: none; COLOR: #355ea0; PADDING-TOP: 0px; BACKGROUND-REPEAT: no-repeat; HEIGHT: 0px
}
H1.block_title A:visited {
	PADDING-RIGHT: 0px; BACKGROUND-POSITION: left top; PADDING-LEFT: 0px; FONT-WEIGHT: bold; FONT-SIZE: 1em; BACKGROUND-IMAGE: none; PADDING-BOTTOM: 3px; MARGIN: 0px; TEXT-TRANSFORM: none; COLOR: #355ea0; PADDING-TOP: 0px; BACKGROUND-REPEAT: no-repeat; HEIGHT: 0px
}
H1.block_title A:link {
	PADDING-RIGHT: 0px; BACKGROUND-POSITION: left top; PADDING-LEFT: 0px; FONT-WEIGHT: bold; FONT-SIZE: 1em; BACKGROUND-IMAGE: none; PADDING-BOTTOM: 3px; MARGIN: 0px; TEXT-TRANSFORM: none; COLOR: #355ea0; PADDING-TOP: 0px; BACKGROUND-REPEAT: no-repeat; HEIGHT: 0px
}
#menu {
	PADDING-RIGHT: 10px; PADDING-LEFT: 10px; FLOAT: left; PADDING-BOTTOM: 10px; WIDTH: 200px; PADDING-TOP: 8px
}
#main {
	PADDING-RIGHT: 20px; PADDING-LEFT: 20px; PADDING-BOTTOM: 0px; MARGIN-LEFT: 200px; WIDTH: auto; PADDING-TOP: 0px
}
DIV.post {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; PADDING-BOTTOM: 0px; PADDING-TOP: 10px
}
DIV.itemdesc {
	FONT-SIZE: 10px; TEXT-TRANSFORM: none; COLOR: #808080; FONT-FAMILY: "Franklin Gothic Book", "trebuchet ms" , "lucida grande" , verdana, arial, sans-serif
}
INPUT {
	backgrond: #fff
}
INPUT {
	VERTICAL-ALIGN: middle; BORDER-TOP-STYLE: none; LINE-HEIGHT: 20px; BORDER-RIGHT-STYLE: none; BORDER-LEFT-STYLE: none; HEIGHT: 20px; BORDER-BOTTOM-STYLE: none
}
BUTTON {
	VERTICAL-ALIGN: middle; BORDER-TOP-STYLE: none; LINE-HEIGHT: 20px; BORDER-RIGHT-STYLE: none; BORDER-LEFT-STYLE: none; HEIGHT: 20px; BORDER-BOTTOM-STYLE: none
}
#btn {
	FONT-WEIGHT: bold; BACKGROUND: repeat-x; COLOR: #fff
}
DIV.comment {
	BORDER-RIGHT: #dddddd 1px dashed; PADDING-RIGHT: 10px; BORDER-TOP: #dddddd 1px dashed; PADDING-LEFT: 0px; PADDING-BOTTOM: 10px; MARGIN: 10px 0px 0px; BORDER-LEFT: #dddddd 1px dashed; PADDING-TOP: 10px; BORDER-BOTTOM: #dddddd 1px dashed; BACKGROUND-COLOR: #fbfbfb
}
DIV.postcontent {
	FONT-WEIGHT: normal; FONT-SIZE: 12px
}
