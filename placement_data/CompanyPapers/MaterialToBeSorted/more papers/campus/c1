7.#include<stdio.h>
#include<conio.h>
main()
{
int i,j,k;
clrscr();
i=2;j=1;k=0;
if((i<0)&&(j<i)&&(k=2))
 printf("\n%d",k);
else
 printf("0");
if((i>2)||(i>1)||(k=1))
 printf("\n%d",k);
else
 printf("1");
getch();
}
