#include<stdio.h>
void main(void);
static int i=50;
int print(int i);
void main(void)
{
	static int i=100;
	while(print(i))
	{
		printf("%d\n",i);
		i--;
	}
}
int print(int x)
{
	static int i=2;
	return(i--);
}



#include<stdio.h>
void main(void);
typedef struct NType
{
	int i;
	char c;
	long x;
} NewType;
void main(void)
{
	NewType *c;
	c=(NewType *)malloc(sizeof(NewType));
	c->i=100;
	c->c='C';
	(*c).x=100L;
	printf("(%d,%c,%4Ld)",c->i,c->c,c->x);
}



#include<stdio.h>
void main(void);
const int k=100;
void main(void)
{
	int a[100];
	int sum=0;
	for(k=0;k<100;k++)
		*(a+k)=k;
	sum+=a[--k];
	printf("%d",sum);
}



