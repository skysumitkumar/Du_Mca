16).
--------------------------------------------------------------

int x;
main()
{
	int x=0;
	{
		int x=10;
		x++;
		change_value(x);
		x++;
		Modify_value();
		printf("First output: %d\n",x);
	}
	x++;
	change_value(x);
	printf("Second Output : %d\n",x);
	Modify_value();
	printf("Third Output : %d\n",x);
}

Modify_value()
{
	return (x+=10);
}

change_value()
{
	return(x+=1);
}





Ans   :   12     1      1




